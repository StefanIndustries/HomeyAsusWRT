  "flow": {
    "triggers": [
      {
        "title": {
          "en": "Came online"
        },
        "hint": {
          "en": "Checks every minute if a device connected to your network"
        },
        "args": [
          {
            "name": "device",
            "type": "device",
            "filter": "driver_id=asus-router"
          }
        ],
        "tokens": [
          {
            "type": "string",
            "name": "name",
            "title": {
              "en": "name"
            },
            "example": {
              "en": "Samsung Phone"
            }
          },
          {
            "type": "string",
            "name": "nickname",
            "title": {
              "en": "nickname"
            },
            "example": {
              "en": "Stefan's Phone"
            }
          },
          {
            "type": "string",
            "name": "ip",
            "title": {
              "en": "ip"
            },
            "example": "192.168.1.1"
          },
          {
            "type": "string",
            "name": "mac",
            "title": {
              "en": "mac"
            },
            "example": "AA:BB:CC:DD:EE:FF"
          }
        ],
        "id": "device-came-online"
      },
      {
        "title": {
          "en": "Went offline"
        },
        "hint": {
          "en": "Checks every minute if a device disconnected from your network"
        },
        "args": [
          {
            "name": "device",
            "type": "device",
            "filter": "driver_id=asus-router"
          }
        ],
        "tokens": [
          {
            "type": "string",
            "name": "name",
