        "title": {
          "en": "WAN status changed"
        },
        "hint": {
          "en": "Checks every 5 minutes if your WAN is still online"
        },
        "args": [
          {
            "name": "device",
            "type": "device",
            "filter": "driver_id=asus-router"
          }
        ],
        "tokens": [
          {
            "type": "boolean",
            "name": "wan_connected",
            "title": {
              "en": "WAN Connected"
            }
          }
        ],
        "id": "wan-connection-status-changed"
      }
