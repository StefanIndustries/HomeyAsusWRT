            },
            "example": {
              "en": "Stefan's Phone"
            }
          },
          {
            "type": "string",
            "name": "ip",
            "title": {
              "en": "ip"
            },
            "example": "192.168.1.1"
          },
          {
            "type": "string",
            "name": "mac",
            "title": {
              "en": "mac"
            },
            "example": "AA:BB:CC:DD:EE:FF"
          }
        ],
        "id": "device-went-offline"
      },
      {
        "title": {
          "en": "External IP changed"
        },
        "hint": {
          "en": "Checks every 5 minutes if the external IP has changed"
        },
        "args": [
          {
            "name": "device",
            "type": "device",
            "filter": "driver_id=asus-router"
          }
        ],
        "tokens": [
          {
            "type": "string",
            "name": "external_ip",
            "title": {
              "en": "External IP"
            }
          }
        ],
        "id": "external-ip-changed"
      },
